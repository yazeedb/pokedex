{
    "extends": "airbnb",
    "rules": {
      "max-len": ["error", 80],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-curly-spacing": [2, { "when": "always" }],
      "comma-dangle": ["error", "never"],
      "react/require-default-props": 0,
      "import/no-unresolved": 0,
      "import/no-absolute-path": 0,
      "import/extensions": 0
    },
    "globals": {
    "__dirname": true,
    "process": true,
    "describe": true,
    "it": true,
    "expect": true,
    "document": true
  }
}
